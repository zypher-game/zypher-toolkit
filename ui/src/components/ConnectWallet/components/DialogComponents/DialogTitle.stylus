
.dialog_title_modalTitleInner
  position: relative
  display: flex
  justify-content: space-between
  align-items: center
  height: 64px
  padding: 15px 20px
  background: #293457;
 
  .dialog_title_title
    font-style: normal
    font-weight: 600
    font-size: 16px
    color: var(--var-text-light)
    line-height: 24px;
  span
    display: block
    cursor: pointer
    :global(.icon)
      size: 24px
      fill: var(--var-text-light)

