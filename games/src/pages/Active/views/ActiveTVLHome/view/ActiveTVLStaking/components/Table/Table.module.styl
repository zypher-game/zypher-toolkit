.title
  margin-top: 30px
  margin-bottom: 10px
  font-size: 18px
  font-weight: 600
  @media screen and (max-width: 830px)
    font-size: 16px
    margin-top: 20px
.fl_tab_header
  p
    font-size: 12px
.fl_tab_body
  cursor pointer
  p
    font-size: 14px
.fl_tab_body_col1
  display: flex
  gap: 10px
  justify-content: flex-start

:global(body .tvlPixelTable_header_table)
  :global(>.pixel_flat_inner)
    padding: 0
    .row
      padding: 16px 30px 

.row
  padding: 16px 40px 20px 
  width: 100%
  display: flex
  >p, >div
    display: inline-flex
    align-items: center;
  .text
    display: inline-flex
    justify-content: flex-start
    align-items: center
    gap: 4px
    p
      line-height: 19px
  >p:nth-child(1),>div:nth-child(1)
    width: 20%
  >p:nth-child(2),>div:nth-child(2)
    width: 14%
  >p:nth-child(3),>div:nth-child(3)
    width: 14%
  >p:nth-child(4),>div:nth-child(4)
    width: 14%
  >p:nth-child(5),>div:nth-child(5)
    width: 18%
.mTable
  display: flex
  flex-direction: column
  gap: 10px
.row_m_head
  display: flex
  justify-content: space-between
  align-items: center
  width: 100%;

.m_col
  display: flex
  justify-content: space-between
  align-items: center
  font-size: 14px
  width: 100%;
  padding: 10px 16px
  .grey
    display: inline-flex
    justify-content: flex-start
    align-items: center
    gap: 4px
    color: #D3D4D7
    font-size: 12px
    line-height: 19px
.row_m_body
  padding: 10px 0
  width: 100%;
.fl_tab_body_col2
  display: flex;
  align-items: center;
  gap: 8px;
  flex-direction: row-reverse;