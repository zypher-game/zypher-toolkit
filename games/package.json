{
  "name": "bingo-frontend",
  "version": "1.0.0",
  "description": "bingo frontend",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node script/init.js && yarn rmdist && cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development node script/init.js && cross-env NODE_ENV=development webpack --config ./config/webpack.prod.js",
    "build:testnet": "cross-env NODE_ENV=testnet node script/init.js && cross-env NODE_ENV=testnet webpack --config ./config/webpack.prod.js",
    "start": "cross-env NODE_ENV=development REACT_APP_CONFIG_ENV=development node script/init.js && cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production node script/init.js && cross-env NODE_ENV=production webpack serve --config ./config/webpack.dev.js",
    "start:testnet": "cross-env NODE_ENV=testnet node script/init.js && cross-env NODE_ENV=testnet webpack serve --config ./config/webpack.dev.js",
    "analyze": "rimraf dist && cross-env NODE_ENV=production node script/init.js && cross-env NODE_ENV=production webpack --analyze --config config/webpack.analyze.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ",
    "prepare": "husky install",
    "web3-token-abi": "node ./config/web3TokenAbi.js",
    "rmdist": "rm -rf ./dist",
    "https": "HTTPS=true SSL_CRT_FILE=$HOME/.cert/cert.pem SSL_KEY_FILE=$HOME/.cert/key.pem npm run start:prod"
  },
  "license": "MIT",
