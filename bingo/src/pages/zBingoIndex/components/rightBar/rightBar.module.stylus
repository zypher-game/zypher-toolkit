.rightBar
  position: fixed
  top: 10%
  right: 40px
  display: flex
  justify-content: right 
  align-items: center
  gap: 40px
  z-index: 9;
  @media screen and (max-width: 830px)
    top: 9%
    right: 16px
    gap: 16px
  &.tgRightBar
    @media screen and (max-width: 830px)
      top: 7%

  img 
    width: 56px;
    @media screen and (max-width: 830px) 
      width: 32px;
