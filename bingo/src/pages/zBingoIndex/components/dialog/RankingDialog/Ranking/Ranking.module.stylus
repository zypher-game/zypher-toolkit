.ranking
  position: relative
  &.myItem:after
    content: ""
    position: absolute;
    background: linear-gradient(180deg, rgba(255,255,255,0) 0%, rgba(0,0,0,0.2) 100%);
    bottom: 0;
    left: 20px;
    width: 94%;
    height: 30px;
    @media screen and (max-width: 830px)
      width: 90%;
  .loadMore
    position: relative
    padding-top: 240px
    display: flex
    justify-content: center
    padding-bottom: 40px
