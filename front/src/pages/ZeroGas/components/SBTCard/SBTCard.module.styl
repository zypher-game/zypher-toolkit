.SBT
  position: relative
  width: 100%
  height: 100%
.smallLogo
  position: absolute
  height: 22px
  bottom: 32px
  right: 70px
  @media screen and (max-width: 830px)
    height: 14px
    bottom: 12%;
    right: 14%;
    z-index 3
.SBTSmallWrap
  position relative
  width: 100%
  height: 100%
.SBTSmallBg
  position relative
  width: 100%
.SBTSmall
  margin: 4px
  position absolute
  width: calc(100% - 8px)
  height: calc(100% - 8px)
  z-index: 2;
  top: 0;
  left: 0;