.gameItemMiddle
  position relative
  background-color: #F1D5A5
  padding: 13px 11px
  .bg
    position absolute
    top: 0;
    left: 0;
    width 100%
    height 100%
    display: flex;
    align-items: self-start;
  .Left
    width 2px
    height: 100%;
    background-color: #9E8E71
    .Left1
      height: calc(100% - 3px)
      border-top: 3px solid #0E2138
      border-bottom: 3px solid #0E2138
    .Left2
      height: 3px
      background-color: #1E4575
  .Middle
    width: 100%
    height: 100%
    border-top: 3px solid #1E4575
    border-bottom: 3px solid #1E4575
    position relative
    .Top
      position absolute
      top: 0
      left: 0
      width: 100%
      .Top01
        height: 3px
        background-color: #D4AD6A
        &:after 
          content: ""
          position absolute
          top: 1px
          right: 13%
          width 12px
          height 3px
          background-color: #F1D5A5
    .Bottom
      position absolute
      bottom: 0
      left: 0
      width: 100%
      .Bottom01
        height: 3px
        background-color: #D4AD6A
        &:after 
          content: ""
          position absolute
          bottom: 2px
          right 50%
          width 15px
          height 3px
          background-color: #F1D5A5
    
  .Right
    background-color: #9E8E71
    width 3px
    height: 100%
    border-top: 3px solid #0E2138
    border-bottom: 3px solid #0E2138
