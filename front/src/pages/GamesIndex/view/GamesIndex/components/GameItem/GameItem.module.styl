
.gameItem
  margin-top: 1vh
  position: relative
  min-height: 83vh
.fr
  display flex
  // justify-content: flex-start
  // flex-wrap: wrap;
  flex-direction: column
  align-items: flex-start;
.icon
  width 44px
  border-radius: 9px
.gameItemCompComingImg
  display: flex
  justify-content: center
  align-items: center
  flex-wrap: wrap
  gap 10px
  cursor auto
.game
  display: flex
  justify-content: flex-start
  align-items: center
  gap: 9px
  h4
    font-size: 14px
    font-weight: 600
    color: #413416
    line-height: 17px
  .content
    display: flex
    justify-content: flex-start
    align-items: center
    gap: 4px
    p
      font-size: 12px
      line-height: 10px
      color: #413416
    img
      width 10px
  .btn
    margin-top: 6px
.btn_label
  font-size: 12px
  line-height: 20px
  padding: 0 10px
.label
  font-size: 12px
  font-weight: 600
  color: #413416
  text-align: center
// bg
.gameItemComp
  display: flex
  align-items: flex-start
  position absolute
  z-index: 99
  transition: scale 0.3s ease
  cursor pointer
  &:hover
    scale: 1.03

  .gameItemMiddle
    height: 80px
    margin-top: 14px
  .gameItemBgRight
    margin-top: 8px

.Bottom_tetris04
  position absolute
  width: 6px
  height: 4px
  right: 6%
  bottom: 2px
  background-color: #D4AD6A
  &:before,
  &:after
    content: ""
    position absolute
  &:before
    width: 3px
    height 4px
    background-color: #D4AD6A
    right: 1px
    bottom: 3px
  &:after
    width: 3px
    height 2px
    background-color: #ECC88A
    right: -3px
    bottom: 1px
