.rank_my
  position: fixed
  bottom: 0
  right: 0
  z-index 9
  width: 100%
  display: flex;
  justify-content: space-between;
  align-items: center;
  .side
    width: 240px;
    @media screen and (max-width: 768px)
      width: 0;

  .rank_my_item
    border-radius: 8px;
    background-color: #1649FF;
    flex: 1;
    margin: 0 40px 40px
    div:first-child
      border-color: rgba(255,255,255,0.2)
    @media screen and (max-width: 768px)
      margin: 0 16px 30px
